@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

<#EventPlace>
    a sh:NodeShape ;
    sh:targetClass schema:Event, schema:MusicEvent, schema:TheaterEvent ; 
    sh:property [
        sh:path schema:location ;
        sh:minCount 1 ;
        sh:severity sh:Violation ;
    ]
.
